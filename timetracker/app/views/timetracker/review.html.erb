<h2>Review my arrivals - <%= @employee.full_name %></h2>
<br>
<% if @error.nil?%>
<table id="review-table" class="display">
<thead>
  <tr>
    <th>Date</th>
    <th>Time</th>
    <th>Type</th>
  </tr>
</thead>
  <tbody>
    <% @tts.each do |tt| %>
      <tr>
        <td><%= tt.date %></td>
        <td><%= tt.time.strftime("%T")  %></td>
        <td><%= tt.inout_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
<h3><%= @error %></h3>
<% end %>
